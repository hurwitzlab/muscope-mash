perl:
	time ./process-dist.pl6 --in=mash-dist.txt --out=out-perl.txt --alias-file=alias.txt

python:
	time ./process-dist.py -i mash-dist.txt -o out-python.txt -a alias.txt

perl-near:
	time ./process-dist.pl6 --in=mash-dist.txt --out=out-perl-near.txt --alias-file=alias.txt --nearness

python-near:
	time ./process-dist.py -i mash-dist.txt -o out-python-near.txt -a alias.txt -n
